<body id="home">

  <!-- Preloader -->
  <div id="preloader">
    <div id="status"></div>
  </div>

  <!-- FullScreen -->
    <div class="intro-header">
    <div class="col-xs-12 text-center abcen1">
      <h1 class="h1_home wow fadeIn" data-wow-delay="0.4s">Rating!</h1>
      <ul class="list-inline intro-social-buttons">
        <li id="download" >
          <a href="/new" class="btn  btn-lg mybutton_standard wow swing wow fadeIn" data-wow-delay="1.2s"><span class="network-name">Start!</span></a>
        </li>
      </ul>
    </div>
        <!-- /.container -->
    <div class="col-xs-12 text-center abcen wow fadeIn">
      <div class="button_down "> 
        <a class="imgcircle wow bounceInUp" data-wow-duration="1.5s"  href="#whatis"></a>
      </div>
    </div>
    </div>


  <!-- NavBar-->
<%= render partial: 'common/header' %>

  <!-- What is -->
  <div id="whatis" class="content-section-b" style="border-top: 0">
    <div class="container">

      <div class="col-md-6 col-md-offset-3 text-center wrap_title">
        <h2>Ranking</h2>
        
      </div>
      
      <div class="row">
      
        <div class="col-sm-4 wow fadeInDown text-center">
